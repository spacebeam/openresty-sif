    lg.print("SOUND VOL:", 65, 102) lg.print(math.floor(config.sfx_volume*10+0.01), 167, 102)
    lg.print("MUSIC VOL:", 65, 115) lg.print(math.floor(config.music_volume*10+0.01), 167, 115)
